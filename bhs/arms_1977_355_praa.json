        "bioghist": {
            "id": "ref3",
            "head": "Biographical/Historical note",
            "p": {
                "value": "Peter (Pieter) Praa (1655-1740) was a prominent landowner in the town of Bushwick in Kings County. He was appointed a Captain in the Bushwick Militia in 1702."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "One photostat copy of a certificate, dated 1702, appointing Peter Praa a Captain in the Bushwick Militia in the regiment of Colonel Jacob Van Cortland. The certificate is issued by Edward Viscount Cornbury, Her Majesty's Captain and Governor in the English Province of New York."
            }
        },

        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "Henry M. Western was a New York City musician. He, and later, his daughters, performed at various concerts and cultural events in Brooklyn."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Three scrapbooks of clippings, photographs, and a variety of ephemera related to the Western family and Brooklyn events, places, and clubs. Some national figures and events are also represented. Materials span the period circa 1880 to 1932."
            }

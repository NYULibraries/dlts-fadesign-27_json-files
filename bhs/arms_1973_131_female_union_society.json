        "bioghist": {
            "id": "ref3",
            "head": "Historical note",
            "p": {
                "value": "The Female Union Society for the Promotion of Sabbath Schools was founded in 1816 by Joanna Graham Bethune, who along with her mother, Isabella Marshall Graham, had been longtime proponents of establishing Sunday Schools in New York City for the purpose of teaching reading and writing to impoverished children. The Society established several nondenominational Sunday schools throughout New York City that taught several thousand students."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "The Female Union Society for the Promotion of Sabbath Schools records consists of a single bound volume dated 1825. Included in the records are a copy of the Society's constitution and by-laws; meeting minutes; lists of members and officers; and accounts."
            }
        },

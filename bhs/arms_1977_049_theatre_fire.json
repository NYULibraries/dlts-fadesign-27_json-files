3e\n                        \u003ccontainer id=\"cid250006\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                        \u003ccontainer parent=\"cid250006\" type=\"Drawer\"\u003e6\u003c/container\u003e\n                        \u003cunitdate normal=\"1876/1877\" type=\"inclusive\"\u003e1876-1877\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref25\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eFinance Committee Records\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid250007\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                        \u003ccontainer parent=\"cid250007\" type=\"Folder\"\u003e7\u003c/container\u003e\n                        \u003cunitdate normal=\"1876/1877\" type=\"inclusive\"\u003e1876-1877\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref26\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eMinutes\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid250008\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                        \u003ccontainer parent=\"cid250008\" type=\"Drawer\"\u003e8\u003c/container\u003e\n                        \u003cunitdate normal=\"1877/1877\"\u003eApril 18, 1877\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref2\" level=\"series\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eSeries 2. Burial Records\u003c/unittitle\u003e\n                    \u003clangmaterial\u003e\n                        \u003clanguage langcode=\"eng\"/\u003e\n                    \u003c/langmaterial\u003e\n                    \u003cunitdate normal=\"1876/1877\" type=\"inclusive\"\u003e1876-1877\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003cscopecontent id=\"ref49\"\u003e\n                    \u003chead\u003eScope and Contents note\u003c/head\u003e\n                    \u003cp\u003eThis series comprises information about the burials of those killed in the fire. The first folder lists those to be buried at the expense of the city, the second is a printed record of the three memorial services held concurrently at different locations on Dec. 10, 1876, and the third folder holds the city’s receipts from undertakers.\u003c/p\u003e\n                \u003c/scopecontent\u003e\n                \u003cc id=\"ref28\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eMemorial Services\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid250010\" type=\"Box\" label=\"Text\"\u003e2\u003c/container\u003e\n                        \u003ccontainer parent=\"cid250010\" type=\"Folder\"\u003e2\u003c/container\u003e\n                        \u003cunitdate normal=\"1876/1876\"\u003eDecember 10, 1876\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref27\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eList of Persons to be Buried at the Expense of the City of Brooklyn\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid250009\" type=\"Box\" label=\"Text\"\u003e2\u003c/container\u003e\n                        \u003ccontainer parent=\"cid250009\" type=\"Folder\"\u003e1\u003c/container\u003e\n                        \u003cunitdate normal=\"1876/1876\"\u003eDecember 1876\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref29\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eUndertakers Receipts\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid250011\" type=\"Box\" label=\"Text\"\u003e2\u003c/container\u003e\n                        \u003ccontainer parent=\"cid250011\" type=\"Folder\"\u003e3\u003c/container\u003e\n                        \u003cunitdate normal=\"1876/1877\" type=\"inclusive\"\u003e1876-1877\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref3\" level=\"series\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eSeries 3. Case Notes\u003c/unittitle\u003e\n                    \u003clangmaterial\u003e\n                        \u003clanguage langcode=\"eng\"/\u003e\n                    \u003c/langmaterial\u003e\n                    \u003cunitdate normal=\"1876/1877\" type=\"inclusive\"\u003e1876-1877\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003cscopecontent id=\"ref48\"\u003e\n                    \u003chead\u003eScope and Contents note\u003c/head\u003e\n                    \u003cp\u003eThe Case Notes section is in three format
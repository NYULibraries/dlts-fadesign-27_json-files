        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "The Brooklyn Revenue Reform Club, founded circa 1879, was an organization that advocated free trade principles. Henry Ward Beecher (1813-1887) was president of the organization, and Thomas G. Shearman (1834-1900), Beecher's lawyer, was its founder."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "One bound scrapbook containing various ephemeral items dating from 1884 to 1887 and relating to the Brooklyn Revenue Reform Club, including clippings from numerous Brooklyn and New York City newspapers, dinner invitations and menus, event announcements and admission tickets, membership applications, circulars, and leaflets."
            }
        },

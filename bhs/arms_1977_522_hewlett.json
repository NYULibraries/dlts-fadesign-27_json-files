        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "William Tryon (1729-1788) was a British soldier who served as Governor of the British provinces of North Carolina (1765-1771) and New York (1771-1780)."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Two handwritten copies of military commission letters issued to Benjamin Hewlett, Jr. by William Tryon in 1772 and 1776. The letters appoint Hewlett to the ranks of Junior Lieutenant Captain and Captain in the Queens County Militia. The author of the copies is not known."
            }
        },

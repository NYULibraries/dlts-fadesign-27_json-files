        "bioghist": {
            "id": "ref7",
            "head": "Biographical Note",
            "p": {
                "value": "Orville B. Ackerly (1842-1923) was Deputy County Clerk of Suffolk County, Long Island from 1871 to 1876, and Suffolk County Clerk from 1877 to 1882. In 1883, he entered the real estate business in New York City and dealt primarily in Long Island properties."
            }
        },
        "scopecontent": {
            "id": "ref8",
            "head": "Scope and Contents",
            "p": {
                "value": "One bound volume containing an index of wills and and letters of administration recorded in Suffolk County, Long Island during the period 1787 to 1880. The index was copied by Orville B. Ackerly from the Suffolk County public records."
            }
        },

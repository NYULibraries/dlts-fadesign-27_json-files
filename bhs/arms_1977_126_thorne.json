        "bioghist": {
            "id": "ref3",
            "head": "Biographical Note",
            "p": {
                "value": "Dr. John S. Thorne was a Brooklyn resident who studied medicine at the University of the State of New York College of Physicians and Surgeons and then practiced in Brooklyn. He served on the Brooklyn Board of Health and at the Brooklyn Dispensary. Thorne's daughter, Elizabeth, married Samuel Haslett, who was the son of Dr. John Haslett, another prominent Brooklyn physician."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection includes various papers of Dr. John S. Thorne spanning the period 1824 to 1836, including correspondence, notes, commissions and appointments, and admission tickets to classes and lectures at the University of the State of New York College of Physicians and Surgeons and the New York Eye Infirmary."
            }
        },

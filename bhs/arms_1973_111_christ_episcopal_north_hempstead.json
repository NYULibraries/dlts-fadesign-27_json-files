        "bioghist": {
            "id": "ref3",
            "head": "Historical Note",
            "p": {
                "value": "The Christ Episcopal Church of North Hempstead, Long Island was organized in 1802. Its building was constructed on the Cow Neck Peninsula of Long Island's North Shore and was completed in 1803. As of 2011, the Church continues to serve North Hempstead's Episcopal community."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "One ledger of the Episcopal Church of North Hempstead spanning the period 1802 to 1834. The ledger consists of meeting minutes regarding the initial formation of the Church, as well as financial accounts and subscription lists."
            }
        },

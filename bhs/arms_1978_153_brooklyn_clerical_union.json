        "bioghist": {
            "id": "ref1",
            "head": "Historical note",
            "p": {
                "value": "The Brooklyn Clerical Union (formerly known as the Clerical Society of the City of Brooklyn) was formed on January 29, 1844 through the efforts of Maurice W. Dwight. The Union's mission was to promote Christian love and confidence among the ministers of the various religious denominations in Brooklyn. The five founding officers were Reverend Maurice W. Dwight, First Reformed Dutch Church, President; Reverend J.L. Hodge, First Baptist Church, Vice-President; Reverend J. Poisal, Methodist (church unknown), Secretary; and Reverend George Duffield, Fifth Presbyterian Church, Treasurer."
            }
        },
        "scopecontent": {
            "id": "ref6",
            "head": "Scope and Contents",
            "p": {
                "value": "Box 2 contains: two minute books dated 1923 to 1932 and 1932 to 1940; and one scrapbook, 1870 to 1934."
            }
        },

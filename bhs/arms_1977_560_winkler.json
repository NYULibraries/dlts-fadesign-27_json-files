        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "Sylvester Winkler operated a bakery at 752 Pacific Street at the corner of Vanderbilt Avenue in Brooklyn. His residence was above his place of business."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents",
            "p": {
                "value": "This collection spans 1854 to 1870 and consists of a mortgage for Sylvester Winkler's bakery on Pacific Street in Brooklyn, and various business and tax receipts and licenses for Winkler's business."
            }
        },

        "bioghist": {
            "id": "ref3",
            "head": "Historical note",
            "p": {
                "value": "Wilmot and Andrews owned a general store and possibly owned rental houses in New York City in the 19th and early 20th centuries."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection consists of two ledgers which belonged to Wilmot and Andrews, each containing lists of daily sales and expenditures dating from 1839-1915. Sales include provisions for families and ships, such as meat, fish, sugar, bread, produce, dairy products, and other goods. Wilmot and Andrews conducted business in New York City."
            }
        },

        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "William H. Lyons and Company was a fine goods importing company in New York City, founded in 1845 by Brooklyn resident William H. Lyons. Partners in the company at various times included William H. Lyons, Warren H. Lyons, Robert Shaw, Morris Soher, Abner S. Haight, James H. Mallory, and Joseph Soher."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection contains various records of William H. Lyons and Company spanning the period 1842 to 1873. Items include articles of partnership, a letter between Robert Shaw and William H. Lyons, and a grocery account of Robert Shaw with Thomas P. Cook."
            }
        },

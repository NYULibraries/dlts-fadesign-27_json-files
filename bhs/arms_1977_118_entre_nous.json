        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "The Entre Nous Dramatic and Literary Association of Brooklyn was an amateur theatrical club formed by residents of southern Brooklyn in 1867. The club split in 1871, with one faction becoming a social club under the name of Entre Nous and another adopting the name of Amaranth and continuing to stage theatrical productions."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Membership roll of Entre Nous Dramatic and Literary Association of Brooklyn. The roll lists each member's name and address. Dated circa 1867."
            }
        },

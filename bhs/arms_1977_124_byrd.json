        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "Richard Evelyn Byrd (1888-1957) was a Rear Admiral in the United States Navy who was awarded the Medal of Honor for his explorations of the polar regions."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection contains materials relating to a reception held at Brooklyn's Hotel St. George in 1930 to honor Rear Admiral Richard Evelyn Byrd. Items include a reception program, a seating arrangement chart, and a photogravure portrait of Byrd."
            }
        },

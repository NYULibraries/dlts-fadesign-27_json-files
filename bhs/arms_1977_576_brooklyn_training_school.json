        "bioghist": {
            "id": "ref2",
            "head": "Historical Note",
            "p": {
                "value": "The Brooklyn Training School and Home for Young Girls (also known as the Protestant Problem Girls School and P.S. 401) was founded circa 1890. Until 1947, it was the only institution of its kind for Protestant girls in New York City."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection includes notes and reports of Elaine Herne, Acting Assistant Principal at the Brooklyn Training School and Home for Young Girls, dating from 1933 to 1947. Notes include lists, evaluations, and assessments of students and teachers."
            }
        },

        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "St. Paul's Free Church was an Episcopal congregation formed by former members of St. Ann's, Brooklyn's earliest Episcopal church. St. Paul's purchased a building on Pearl Street (later Concord Street), but suspended services in 1840. It reopened later as Calvary Church, but had ceased to exist by 1862."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Account book of St. Paul's Free Church for the period 1834 to 1840, including treasurer's accounts of income and expenditures; accounts of the minister, vestry, and sexton; and lists of special donations."
            }
        },

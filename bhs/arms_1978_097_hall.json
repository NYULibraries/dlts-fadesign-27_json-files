        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "The Reverend Charles Henry Hall (1820-1895) was rector of the Holy Trinity Church, located at Clinton and Montague Streets in Brooklyn, N.Y., from January 1869 until his death. On January 11, 1887, he led the funeral service of his friend and prominent Brooklyn clergyman Henry Ward Beecher."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Letter requesting Charles H. Hall to speak at the 70th meeting of the American Colonization Society in Washington, D.C. Dated 1886."
            }
        },

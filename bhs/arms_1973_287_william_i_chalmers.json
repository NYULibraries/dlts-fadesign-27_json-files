        "bioghist": {
            "id": "ref2",
            "head": "Biographical/Historical note",
            "p": {
                "value": "The Reverend William I. Chalmers served as pastor of the Congregational church in Riverhead, New York during the late 19th century. Chalmers wrote and published books on the history of the town of Riverhead and the Congregational church in Aquebogue, New York."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "Original handwritten manuscript titled \n                \u003cemph render=\"italic\"\u003eThe Suffolk Association of Congregational churches and ministers and its relation to antecedent bodies\u003c/emph\u003e. This paper was read at the association's 25th anniversary by William I. Chalmers in 1898."
            }
        },

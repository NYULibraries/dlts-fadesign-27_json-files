        "bioghist": {
            "id": "ref5",
            "head": "Historical Note",
            "p": {
                "value": "In 1894, the Town of Huntington, Long Island, New York erected a granite monument in memory of Captain Nathan Hale, who had landed at Huntington in 1776 on an intelligence-gathering mission for the Continental Army that ultimately resulted in his capture and execution by the British Army."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "One certificate, dated May 17, 1894, stating that Morris Patterson Harris has contributed five dollars toward the erection of the Nathan Hale Memorial in Huntington, Long Island."
            }
        },

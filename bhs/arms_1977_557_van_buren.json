        "bioghist": {
            "id": "ref5",
            "head": "Biographical Note",
            "p": {
                "value": "Ansel Hayes Van Buren (1846-1910) married Idelle Newell on November 11, 1884. Ansel was an acomplished musician and salesman with various piano houses in Brooklyn. He and Idelle lived in the Brooklyn neighborhood of Flatbush."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Bond of Ansel and Idelle Van Buren of $21,000 to Henry Lankenau. Signed by a Kings County notary public and dated 1891."
            }

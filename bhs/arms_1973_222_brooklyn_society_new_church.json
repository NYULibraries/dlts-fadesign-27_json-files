        "bioghist": {
            "id": "ref3",
            "head": "Historical Note",
            "p": {
                "value": "The Brooklyn Society of the New Church was a Swedenborgian church organized in the 1850s. In 1869, the church purchased land at Clark and Monroe Streets that had formerly belonged to the Universalist Church of the Restoration."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "Records relating to the Brooklyn Society of the New Church's purchase of property at Clark and Monroe Streets, including subscription lists, minutes, mortgages, receipts, and correspondence. Documents span the period 1868 to 1926."
            }
        },

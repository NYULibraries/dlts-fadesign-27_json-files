        "bioghist": {
            "id": "ref3",
            "head": "Biographical Note",
            "p": {
                "value": "Lillian A. de Severinus was an artist and the head of the De Severinus Art School (also known as the De Severinus Academy of Fine and Applied Art) in Brooklyn."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection contains various materials spanning the period 1898 to 1941 and relating to Brooklyn-based artist Lillian A. de Severinus, the De Severinus Art School, and art in Brooklyn. Items include two scrapbooks of clippings and photographs, as well as loose pamphlets, photographs, programs, trade and exhibition cards, and one page of a diary."
            }
        },

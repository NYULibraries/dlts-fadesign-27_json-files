        "dsc": {
            "value": "\n            \u003cc id=\"ref17\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eBrooklyn Property Records.\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443001\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443001\" type=\"Folder\"\u003e1\u003c/container\u003e\n                    \u003cunitdate normal=\"1785/1828\" type=\"inclusive\"\u003e1785-1828\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref18\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e2 items, including a property deed and property lease.\u003c/p\u003e\n                \u003c/odd\u003e\n                \u003codd id=\"ref40\"\u003e\n                    \u003chead\u003eGeneral note\u003c/head\u003e\n                    \u003cp\u003eMoved to oversize box: one property deed, 1785 and one property lease, 1828.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref19\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eAccount Books (1 of 2).\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443002\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443002\" type=\"Folder\"\u003e2\u003c/container\u003e\n                    \u003cunitdate normal=\"1810/1828\" type=\"inclusive\"\u003e1810-1828\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref20\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e4 items, including account books from The Long Island Bank, Merchants Bank, and Phenix Bank.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref21\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eAccount Books (2 of 2).\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443003\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443003\" type=\"Folder\"\u003e3\u003c/container\u003e\n                    \u003cunitdate normal=\"1829/1851\" type=\"inclusive\"\u003e1829-1851\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref22\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e5 items, including account books from the Long Island Bank.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref23\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eBills and Receipts (1 of 2).\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443004\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443004\" type=\"Folder\"\u003e4\u003c/container\u003e\n                    \u003cunitdate normal=\"1824/1844\" type=\"inclusive\"\u003e1824-1844\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref24\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003eItems include receipts for taxes, postage, subscriptions, bills for blacksmiths, clothing, doctors, fuel, laborers, mill supplies, wood, and hardware for house construction.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref26\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eBills and Receipts (2 of 2).\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443005\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443005\" type=\"Folder\"\u003e5\u003c/container\u003e\n                    \u003cunitdate normal=\"1833/1844\" type=\"inclusive\"\u003e1833-1844\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref27\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003eItems include bills and payment receipts for blacksmiths, fuels, laborers, and mill supplies.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref28\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eBusiness Records.\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443006\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443006\" type=\"Folder\"\u003e6\u003c/container\u003e\n                    \u003cunitdate normal=\"1797/1842\" type=\"inclusive\"\u003e1797-1842\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref29\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e9 items, including mill real estate plans and license to use patented milling machinery.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref30\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003ePersonal Papers.\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443007\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443007\" type=\"Folder\"\u003e7\u003c/container\u003e\n                    \u003cunitdate normal=\"1800/1843\" type=\"inclusive\"\u003e1800-1843\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref31\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e10 items, including bill of sale for African-American boy, property sale contracts, and estate matters of Samuel Denton.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref32\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eReceipt Book/Pew Rental Lists.\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443008\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443008\" type=\"Folder\"\u003e8\u003c/container\u003e\n                    \u003cunitdate normal=\"1817/1826\" type=\"inclusive\"\u003e1817-1826\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref33\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e2 items, including a list of pew rentals and owners of the Reformed Dutch Church.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref34\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eWilliam Skidmore Papers.\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid443009\" type=\"Box\" label=\"Text\"\u003e1\u003c/container\u003e\n                    \u003ccontainer parent=\"cid443009\" type=\"Folder\"\u003e9\u003c/container\u003e\n                    \u003cunitdate normal=\"1817/1818\" type=\"inclusive\"\u003e1817-1818\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003codd id=\"ref35\"\u003e\n                    \u003chead\u003eContent note\u003c/head\u003e\n                    \u003cp\u003e4 items, not directly related to Denton.\u003c/p\u003e\n                \u003c/odd\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref41\" level=\"file\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eOversize materials.\u003c/unittitle\u003e\n                    \u003ccontainer id=\"cid478001\" type=\"Box\" label=\"Text\"\u003e2\u003c/container\u003e\n                    \u003ccontainer parent=\"cid478001\" type=\"Folder\"\u003e1\u003c/container\u003e\n                \u003c/did\u003e\n            \u003c/c\u003e\n        "

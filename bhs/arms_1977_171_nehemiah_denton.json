        "bioghist": {
            "id": "ref1",
            "head": "Biographical notes",
            "p": {
                "value": "Denton died in 1844. He left as his heir his grandson, Nehemiah Denton Wilkins. He was buried in the family plot at the Prospect Cemetery in Jamaica."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents",
            "p": {
                "value": "Three items refer to Denton's brother, Samuel, who died in 1799. Three others concern William Skidmore of Greenwich, a cousin."
            }
        },

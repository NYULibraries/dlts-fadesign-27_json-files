        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "The Wyandot Baseball Club of Flatlands Neck was established in the town of Flatlands in Kings County in 1859 for the purpose of organizing recreational baseball games with other baseball clubs in nearby towns."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection consists of a minute book of the Wyandot Baseball Club of Flatlands Neck in Brooklyn for the period 1859 to 1866. The book includes minutes from the Club's meetings, membership and attendance lists, scores and statistics from games played, a draft of a speech, clippings, and various notes. Also included is a loose undated membership roll marking each members' attendance at meetings throughout the year."
            }
        },

        "bioghist": {
            "id": "ref3",
            "head": "Biographical/Historical note",
            "p": {
                "value": "Bancroft Gherardi (1832-1903 was born in Louisiana. He entered the United States Naval Academy in 1852, was appointed Commandant of the Brooklyn Navy Yard in 1884."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "The farewell address Bancroft Gherardi, Rear Admiral and Commandant of the Brooklyn Navy Yard, upon his retirement from the United States Navy. The address is dated November 8, 1894."
            }
        },

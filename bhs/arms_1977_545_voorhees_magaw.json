        "bioghist": {
            "id": "ref3",
            "head": "Biographical Note",
            "p": {
                "value": "Adriana Voorhees Magaw (1795-1831) was a resident of Gravesend, Kings County and married Van Brunt Magaw (1783-1831) on November 2, 1811 in the Reformed Dutch Church of Flatbush. Their first child, Mary Elizabeth Magaw, was born on October 5, 1812, and they had three more daughters and three sons."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Probate of the last will and testament of Adriana Voorhees Magaw, dated June 16, 1831."
            }
        },

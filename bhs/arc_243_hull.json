        "bioghist": {
            "id": "ref5",
            "head": "Biographical Note",
            "p": {
                "value": "Dr. A. Cooke Hull (1818-1868) was a resident of Brooklyn, a physician, a trustee of Public Schools 8 and 11 in Brooklyn, and a founder of the Long Island Historical Society (now the Brooklyn Historical Society)."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents",
            "p": {
                "value": "This collection contains a petition endorsing A. Cooke Hull as a candidate for alderman of Brooklyn's Third Ward, dated 1865, and a memorial book for Hull, presented on July 7, 1868 by the Board of Education of the City of Brooklyn."
            }
        },

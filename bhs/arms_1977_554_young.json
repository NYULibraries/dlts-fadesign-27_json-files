        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "Reverend David Young (1719-1752) was born in Southold, Long Island and graduated from Yale College in 1741. He was ordained in either Setauket or Brookhaven, Long Island in 1742."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection includes various materials spanning 1891 to 1896 and relating to Reverend David Young, including originals of some of Reverend David Young's sermons; research notes on important dates in Young's life and sermons; a copy of a letter from Jonathan Barber to Young's wife sending condolences upon Young's death; a copy of an excerpt from \n                \u003cemph render=\"italic\"\u003eA History of the Presbyterian Church in America\u003c/emph\u003e containing a biographical sketch of Young; correspondence concerning Young's research; and an unidentified sketch map."
            }
        },

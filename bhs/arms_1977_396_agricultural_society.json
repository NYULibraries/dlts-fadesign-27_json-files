        "bioghist": {
            "id": "ref3",
            "head": "Historical Note",
            "p": {
                "value": "The Agricultural Society of Kings County was established by major landowners Jeremiah Johnson, Hendrick Lott, Leffert Lefferts, Jeremiah Lott, John Terhune, and John Vanderveer between December 27, 1806 and March 2, 1807. The focus of the Society was to promote agriculture, husbandry, and rural affairs."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "One multipage document containing the Agricultural Society of Kings County's constitution, bylaws, list of officers, goals, dues list, and meeting minutes, dated March 1807."
            }
        },

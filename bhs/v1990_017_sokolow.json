        "bioghist": {
            "id": "ref8",
            "head": "Biographical note",
            "p": {
                "value": "Isobel A. Sokolow (nee Folb) was a professional artist who lived and worked in New York, Florida, and Italy. Working under the name Isobel Folb Sokolow, her work was exhibited in museums and galleries both nationally and internationally. She died in 2006."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "Three black-and-white photographs, circa 1950s, showing Isobel A. Folb (later Sokolow) at and near Erasmus Hall High School in the Flatbush neighborhood of Brooklyn."
            }
        },

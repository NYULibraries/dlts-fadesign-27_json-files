        "bioghist": {
            "id": "ref7",
            "head": "Historical note",
            "p": {
                "value": "Revolutionary Cemetery, also referred to as Barkaloo Cemetery, is the smallest cemetery in Brooklyn. Located at Narrows Avenue and Mackay Place in the Bay Ridge neighborhood of Brooklyn, the cemetery is the resting place of well-known Dutch families in Brooklyn, as well as Revolutionary War veterans."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "Three photos by Tom Prendergast, dated 1983, of the Revolutionary Cemetery in the Bay Ridge neighborhood of Brooklyn. Photographs show tombstones and memorials for members of prominent Brooklyn families, including the Barkaloo, Suydam, and Cortelyou families."
            }
        },

        "bioghist": {
            "id": "ref4",
            "head": "Biographical Note",
            "p": {
                "value": "Benjamin Harrison (1833-1901) was a Republican politician who served as the 23rd President of the United States from 1889 to 1893. He was both preceded and succeeded in office by Democrat Grover Cleveland."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "One letter from Benjamin Harrison to George C. Baker of Brooklyn, N.Y., thanking Baker and the Kensington Campaign Club for their support in Harrison's presidential campaign. The letter is dated August 9, 1888."
            }
        },

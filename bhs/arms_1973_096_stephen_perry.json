        "bioghist": {
            "id": "ref3",
            "head": "Biographical note",
            "p": {
                "value": "Dr. Stephen Perry was a physician who lived and worked in Northport, Long Island, New York during the mid- and late-nineteenth century."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "The Stephen Perry account book consists of a single volume and spans the period 1840 to 1884. It lists Dr. Perry's financial transactions for the period 1840 to 1870, and also includes copies of several birth and death certificates pasted into the volume. These documents span the period 1880 to 1884 and are signed by Dr. Perry."
            }
        },

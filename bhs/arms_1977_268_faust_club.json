        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "The Faust Club was formed in 1872 as a social club for Brooklyn artists, actors, musicians, and journalists. The Club was located at a house on Remsen Street, but dissolved in 1875 due to depleted finances."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection includes materials documenting the activities of the Faust Club from 1872 to 1875, including a minute book, correspondence, a directory with lists of members and committees, and an account book with a name index."
            }
        },

        "bioghist": {
            "id": "ref4",
            "head": "Historical Note",
            "p": {
                "value": "The Centennial Celebration of the Evacuation of the City of New-York by the British was held in 1883 to commemorate the evacuation of British forces from New York after their defeat by the Continental Army in the Revolutionary War."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection contains programs, newspapers and periodicals, posters, and souvenirs relating to the Centennial Celebration of the Evacuation of the City of New-York by the British in 1883, and minutes of the celebration's organizing committee."
            }
        },

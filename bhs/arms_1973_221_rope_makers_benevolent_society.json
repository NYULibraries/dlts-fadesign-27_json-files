        "bioghist": {
            "id": "ref2",
            "head": "Historical Note",
            "p": {
                "value": "The Rope Makers' Benevolent Society of Brooklyn was a fraternal organization organized in 1821 and incorporated in 1825. Its aim was to aid widows, orphans, and distressed members. Ropemaking was one of Brooklyn's largest industries in the 19th century."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection consists of one record book containing the printed articles of incorporation and rules and regulations of the Rope Makers' Benevolent Society of Brooklyn, as well as a handwritten roll of members. Also included are a photographic negative and print of a cloth emblem of the Society."
            }
        },

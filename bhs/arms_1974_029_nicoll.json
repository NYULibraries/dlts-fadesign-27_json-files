        "bioghist": {
            "id": "ref2",
            "head": "Biographical Note",
            "p": {
                "value": "William Nicolls (1657-1723) was a lawyer and politician. He was appointed Queens County Clerk in 1683 and became Attorney General of the Province of New York in 1687. In 1701 he represented Suffolk County in the Assembly and in 1702 became Speaker."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "This collection contains 11 grants issued to William Nicoll for land in the Province of New York, many relating to the town of Islip in Suffolk County."
            }
        },

         \u003ccontainer id=\"aspace_b680ed6d0ad4d8af3eb099439aa64423\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_b680ed6d0ad4d8af3eb099439aa64423\" type=\"Folder\"\u003e5\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_ref21\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eWeiner, Rose to Beard, Mary\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1942/1942\" type=\"inclusive\"\u003e1942 September 24\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_b5add54ca017233332c9504e02f82141\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_b5add54ca017233332c9504e02f82141\" type=\"Folder\"\u003e6\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n      \u003c/c\u003e\n      \u003cc id=\"aspace_ref22\" level=\"series\"\u003e\n        \u003cdid\u003e\n          \u003cunittitle\u003eSeries II: Subject Files\u003c/unittitle\u003e\n          \u003cunitdate normal=\"1933/1995\" type=\"inclusive\"\u003e1933-1995\u003c/unitdate\u003e\n        \u003c/did\u003e\n        \u003cscopecontent id=\"aspace_c3b37a0c24f88b601170471fba31cd23\"\u003e\n          \u003chead\u003eScope and Contents\u003c/head\u003e\n          \u003cp\u003eSeries II: Subject Files, 1933-1995, contains Weiner's certificate of U.S. citizenship,\n            official documents from her service in the Spanish Civil War, certificates that\n            registered her for nursing in New York, and collected publications related to the\n            Veterans of the Abraham Lincoln Brigades. Also included are photocopies of a Weiner\n            family scrapbook, from which the documents in this collection were separated.\u003c/p\u003e\n        \u003c/scopecontent\u003e\n        \u003cc id=\"aspace_ref23\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eCertificate of U.S. Citizenship\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1933/1933\" type=\"inclusive\"\u003e1933 February 6\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_b5541d400e9f22c44e5d7828359a65e8\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_b5541d400e9f22c44e5d7828359a65e8\" type=\"Folder\"\u003e7\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_ref24\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eCertificates for Nursing (U.S.)\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1932/1971\" type=\"inclusive\"\u003e1932, 1971\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_67ad5ef4ad607ac905bbeb2c1c8d251e\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_67ad5ef4ad607ac905bbeb2c1c8d251e\" type=\"Folder\"\u003e8\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_ref25\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eCertificate of Service in Spain and Letter of Thanks\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1938/1938\" type=\"inclusive\"\u003e1938 November - December\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_5621d0b30b0e3c2dda247e795b4dec58\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_5621d0b30b0e3c2dda247e795b4dec58\" type=\"Folder\"\u003e9\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_ref26\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eScrapbook: Photocopy\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1920/1999\" type=\"inclusive\"\u003ecirca 1920s-1990s\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_ba26047596a09294b5d644b249f6d444\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_ba26047596a09294b5d644b249f6d444\" type=\"Folder\"\u003e10\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_ref27\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eReal Estate Transaction\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1955/1955\" type=\"inclusive\"\u003e1955 August 30\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_b5782160b216d5eab101fe201c9d536a\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_b5782160b216d5eab101fe201c9d536a\" type=\"Folder\"\u003e11\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_ref28\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eWeiner, Rose: Obituary\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1995/1995\" type=\"inclusive\"\u003e1995 December\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_cf4d8e961bafd456290ec22da6607f46\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_cf4d8e961bafd456290ec22da6607f46\" type=\"Folder\"\u003e12\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n      \u003c/c\u003e\n      \u003cc id=\"aspace_19802b15bf77d332a1391aef1f49168a\" level=\"series\"\u003e\n        \u003cdid\u003e\n          \u003cunittitle\u003eSeries III: Photographs\u003c/unittitle\u003e\n          \u003cunitdate normal=\"1920/1995\" type=\"inclusive\"\u003e1920-1995\u003c/unitdate\u003e\n        \u003c/did\u003e\n        \u003cscopecontent id=\"aspace_034ab54f1de70a8f7f3575915551a1e6\"\u003e\n          \u003chead\u003eScope and Contents\u003c/head\u003e\n          \u003cp\u003eSeries III: Photographs, 1920-1995 contains photographs from Rose Weiner’s scrapbook\n            including images of her childhood, family, and as a student at Montefiore Hospital\n            School of Nursing. There is one image of Weiner’s father in the United States prior to\n            the family’s arrival in the United States, an image of mother Eva Rubenstein with Rose,\n            Dora, Milton, Sam and Florence in Lyubar, Russia, and an image of the Weiner’s neighbors\n            and cousins visiting the grave of Alexi Weiner, who was killed in a Russian pogrom.\n            Additional photographs document the American Medical Bureau’s work during the Spanish\n            Civil War including images of Jaime de Guzman, Captain T. Blin, medical personnel, and\n            unidentified troops fighting for the Spanish Republican Army.\u003c/p\u003e\n        \u003c/scopecontent\u003e\n        \u003cc id=\"aspace_0b948a3385b2a836bee12c4f52db4212\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u003eAmerican Medical Bureau\u003c/unittitle\u003e\n            \u003cunitdate normal=\"1937/1938\" type=\"inclusive\"\u003ecirca 1937-1938\u003c/unitdate\u003e\n            \u003ccontainer id=\"aspace_581511b13a56213affcae8f3f0db618e\"\n              label=\"Mixed Materials (31142063292570)\" type=\"Box\"\u003e1\u003c/container\u003e\n            \u003ccontainer parent=\"aspace_581511b13a56213affcae8f3f0db618e\" type=\"Folder\"\u003e17\u003c/container\u003e\n          \u003c/did\u003e\n        \u003c/c\u003e\n        \u003cc id=\"aspace_899e639dede4522c760006b6e37d607c\" level=\"file\"\u003e\n          \u003cdid\u003e\n            \u003cunittitle\u0
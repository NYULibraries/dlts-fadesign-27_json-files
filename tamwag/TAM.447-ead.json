        "bioghist": {
            "id": "ref4",
            "head": "Historical/Biographical Note",
            "p": {
                "value": "Camp Higley Hill was a summer camp in Vermont (1946-1964) owned and operated by Max and Grace Granich, the latter having served as the personal secretary to Earl Browder, Communist Party, USA General Secretary from 1930 to 1945."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "The collection includes correspondence and publications produced by the camp regarding activities there. PLEASE DIRECT FURTHER INQUIRIES TO: tamiment.wagner@nyu.edu"
            }
        },

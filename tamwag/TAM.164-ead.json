        "bioghist": {
            "id": "ref3",
            "head": "Historical/Biographical Note",
            "p": {
                "value": "David Weiner (b. ca. 1912) was a member of the Teachers Union of New York, and subsequently, the United Federation of Teachers, where he was a member of the left-wing opposition group the Teachers Action Caucus. Weiner also was active in the retired teachers chapter of the UFT, and at one time edited its newsletter."
            }
        },
        "scopecontent": {
            "id": "ref2",
            "head": "Scope and Content Note",
            "p": {
                "value": "This unprocessed collection contains material related to the UFT and its retired teachers chapter, the Teachers Action Caucus, and public education-related topics. PLEASE DIRECT FURTHER INQUIRIES TO: tamiment.wagner@nyu.edu"
            }
        },

        "bioghist": {
            "id": "ref2",
            "head": "Historical/Biographical Note",
            "p": {
                "value": "United Automobile Workers, District 65 and the Office and Professional Employees International Union, Local 153 led an unsuccessful clerical workers organizing campaign at New York University, beginning in the fall of 1969 and continuing through the strike of April/May 1970, the election of May 15, 1970, the New York State Labor Relations Board and the National Labor Relations Board decisions, and another election in 1971."
            }
        },
        "scopecontent": {
            "id": "ref1",
            "head": "Scope and Contents note",
            "p": {
                "value": "Contents of the collection include: printed ephemera produced by the organizing campaign and student supporters, statements and press releases by the university, picket duty material, and NLRB and SLRB documents. PLEASE DIRECT FURTHER INQUIRIES TO: tamiment.wagner@nyu.edu"
            }
        },

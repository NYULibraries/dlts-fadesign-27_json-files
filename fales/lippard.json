      \u003cc id=\"ref39\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eCorrespondence\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid16715006\" type=\"Box\" label=\"Mixed materials (31142050716896)\"\u003e4\u003c/container\u003e\n                        \u003cunitdate normal=\"1980/1994\" type=\"inclusive\"\u003e1980-1994; undated\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                    \u003codd id=\"ref53\"\u003e\n                        \u003chead\u003eGeneral note\u003c/head\u003e\n                        \u003cp\u003eFolders 1-11.\u003c/p\u003e\n                    \u003c/odd\u003e\n                \u003c/c\u003e\n            \u003c/c\u003e\n            \u003cc id=\"ref4\" level=\"series\"\u003e\n                \u003cdid\u003e\n                    \u003cunittitle\u003eSeries IV: Printed Material\u003c/unittitle\u003e\n                    \u003clangmaterial\u003e\n                        \u003clanguage langcode=\"eng\"/\u003e\n                    \u003c/langmaterial\u003e\n                    \u003cunitdate normal=\"1968/1996\" type=\"inclusive\"\u003e1968-1996; undated\u003c/unitdate\u003e\n                \u003c/did\u003e\n                \u003cscopecontent id=\"ref74\"\u003e\n                    \u003chead\u003eScope and Content\u003c/head\u003e\n                    \u003cp\u003eSeries comprises miscellaneous printed material found either foldered in unlabeled folders or loose in boxes. Materials are almost exclusively gallery announcements, invitations, press releases, and exhibition catalogs. Series contains some correspondence, mostly postcard announcements, left in place to preserve original order. Some items in this series are stamped \"RECEIVED; [date]; LEO LERMAN.\"\n                        \u003clb/\u003e\n                        \u003clb/\u003eMaterial in this series is arranged chronologically by earliest date on folder.\u003c/p\u003e\n                \u003c/scopecontent\u003e\n                \u003cc id=\"ref40\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003ePrinted Material\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid16715007\" type=\"Box\" label=\"Mixed materials (31142050716896)\"\u003e4\u003c/container\u003e\n                        \u003cunitdate normal=\"1962/1992\" type=\"inclusive\"\u003e1962-1992; undated\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                    \u003codd id=\"ref54\"\u003e\n                        \u003chead\u003eGeneral note\u003c/head\u003e\n                        \u003cp\u003eFolders 12-25.\u003c/p\u003e\n                    \u003c/odd\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref41\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003ePrinted Material\u003c/unittitle\u003e\n                        \u003clangmaterial\u003e\n                            \u003clanguage langcode=\"eng\"/\u003e\n                        \u003c/langmaterial\u003e\n                        \u003ccontainer id=\"cid16715008\" type=\"Box\" label=\"Mixed materials (31142050716854)\"\u003e5\u003c/container\u003e\n                        \u003cunitdate normal=\"1984/1992\" type=\"inclusive\"\u003e1984-1992; undated\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref42\" level=\"file\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003ePrinted Material\u003c/unittitle\u003e\n        
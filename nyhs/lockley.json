ool children\u003c/unittitle\u003e\n                            \u003ccontainer id=\"cid499596\" type=\"Box\" label=\"Mixed Materials\"\u003e2\u003c/container\u003e\n                            \u003ccontainer parent=\"cid499596\" type=\"Folder\"\u003e4\u003c/container\u003e\n                            \u003cunitdate\u003e1952 Oct.\u003c/unitdate\u003e\n                        \u003c/did\u003e\n                    \u003c/c\u003e\n                    \u003cc id=\"ref29\" level=\"otherlevel\"\u003e\n                        \u003cdid\u003e\n                            \u003cunittitle\u003eLetters to Lockley\u003c/unittitle\u003e\n                            \u003ccontainer id=\"cid499595\" type=\"Box\" label=\"Mixed Materials\"\u003e2\u003c/container\u003e\n                            \u003ccontainer parent=\"cid499595\" type=\"Folder\"\u003e5\u003c/container\u003e\n                            \u003cunitdate\u003e1953\u003c/unitdate\u003e\n                        \u003c/did\u003e\n                    \u003c/c\u003e\n                    \u003cc id=\"ref30\" level=\"otherlevel\"\u003e\n                        \u003cdid\u003e\n                            \u003cunittitle\u003eLetters to Lockley\u003c/unittitle\u003e\n                            \u003ccontainer id=\"cid499594\" type=\"Box\" label=\"Mixed Materials\"\u003e2\u003c/container\u003e\n                            \u003ccontainer parent=\"cid499594\" type=\"Folder\"\u003e6\u003c/container\u003e\n                            \u003cunitdate\u003e1954-1955\u003c/unitdate\u003e\n                        \u003c/did\u003e\n                    \u003c/c\u003e\n                    \u003cc id=\"ref31\" level=\"otherlevel\"\u003e\n                        \u003cdid\u003e\n                            \u003cunittitle\u003eLetters to Lockley, Undated and Fragments (also Alienated Envelopes)\u003c/unittitle\u003e\n                            \u003ccontainer id=\"cid499593\" type=\"Box\" label=\"Mixed Materials\"\u003e2\u003c/contai
             \u003c/did\u003e\n                    \u003cscopecontent id=\"ref27\"\u003e\n                        \u003chead\u003eScope and Contents Note\u003c/head\u003e\n                        \u003cp\u003eJ. Bachmann, artist. J. Bien, lithographer. Published by Edward Fuerster.\u003c/p\u003e\n                    \u003c/scopecontent\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref28\" level=\"item\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eBirds Eye View of New York and Environs\u003c/unittitle\u003e\n                        \u003ccontainer id=\"cid3795001\" type=\"Folder\" label=\"Graphic materials\"\u003eFF2\u003c/container\u003e\n                        \u003cunitdate\u003e1865\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                    \u003cscopecontent id=\"ref29\"\u003e\n                        \u003chead\u003eScope and Contents Note\u003c/head\u003e\n                        \u003cp\u003eJ. Bachmann, artist. J. Bien, printer. Published by John Bachmann, 76 Nassau Street, New York.\u003c/p\u003e\n                    \u003c/scopecontent\u003e\n                \u003c/c\u003e\n                \u003cc id=\"ref30\" level=\"item\"\u003e\n                    \u003cdid\u003e\n                        \u003cunittitle\u003eView of Central Park, New York\u003c/unittitle\u003e\n                        \u003ccontainer id=\"cid3795003\" type=\"Folder\" label=\"Graphic materials\"\u003eFF2\u003c/container\u003e\n                        \u003cunitdate\u003e[1875]\u003c/unitdate\u003e\n                    \u003c/did\u003e\n                    \u003cscopecontent id=\"ref31\"\u003e\n                        \u003chead\u003eScope and Contents Note\u003c/head\u003e\n                        \u003cp\u003eView looking southeast. John Bachmann, artist. H. Bencke, lithographer.\u003c/p\u003e\n                    \u003c/scopecontent\u003e\n                \u003c/c\u003e\n                \u003cc i